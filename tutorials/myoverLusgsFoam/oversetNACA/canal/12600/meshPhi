FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "12600";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];
oriented        oriented;


internalField   uniform 0;

boundaryField
{
    topAndBottom
    {
        type            calculated;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
        value           nonuniform 0();
    }
    oversetPatch
    {
        type            calculated;
        value           uniform 0;
    }
    wallsWall
    {
        type            calculated;
        value           uniform 0;
    }
}


